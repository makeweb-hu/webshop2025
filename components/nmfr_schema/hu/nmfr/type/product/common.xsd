				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRUCK">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="TRUCK"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRAIL">
