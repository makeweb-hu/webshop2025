	<xs:complexType name="CategoryListType">
		<xs:sequence>
			<xs:element name="category" type="CategoryType" maxOccurs="unbounded"></xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BasicVehicleType">
		<xs:sequence>
			<xs:element name="licensePlateNumber" type="common:LicensePlateNumberType"/>
			<xs:element name="countryCode" type="common:CountryCodeType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VehicleType">
		<xs:annotation>
			<xs:documentation>Regisztrált járműről fixen tárolható adatok</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BasicVehicleType">
				<xs:sequence>
					<xs:element name="category" type="CategoryType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CostInfoType">
		<xs:sequence>
			<xs:element name="cost" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>A tranzakció költsége!</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
